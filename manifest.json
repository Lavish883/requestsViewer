{
    "manifest_version": 2,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "tabs",
        "activeTab",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["content.js"],
        "persistent": true
    }
  }